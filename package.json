{
	"name": "backend",
	"version": "1.0.2",
	"description": "",
	"author": "",
	"private": true,
	"license": "ISC",
	"scripts": {
		"build": "nest build api",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start api",
		"start:dev": "nest start api --debug --watch",
		"start:debug": "nest start api --debug --watch",
		"start:prod": "node build/apps/api/main.js",
		"lint": "eslint . --ext=.ts",
		"lint-fix": "eslint . --ext .ts --fix;",
		"lint:biome": "biome check .",
		"format:biome": "biome format . --write",
		"clean:db": "ts-node -r tsconfig-paths/register src/seeds/cleanDb.ts",
		"test": "VITE_CJS_IGNORE_WARNING=1 vitest run",
		"test:watch": "VITE_CJS_IGNORE_WARNING=1 vitest",
		"test:cov": "VITE_CJS_IGNORE_WARNING=1 vitest run --coverage",
		"test:debug": "VITE_CJS_IGNORE_WARNING=1 vitest --inspect-brk --no-threads",
		"test:e2e": "VITE_CJS_IGNORE_WARNING=1 vitest run --config vitest.e2e.config.ts",
		"typeorm:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create src/migrations",
		"typeorm:generate": "./scripts/typeorm-generate.sh",
		"typeorm:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./data-source.ts",
		"typeorm:revert": "ts-node -r tsconfig-paths/register ./data-source.ts migration:revert",
		"typeorm": "typeorm-ts-node-commonjs",
		"seed": "ts-node -r tsconfig-paths/register src/seeds/seed.ts",
		"commit": "npx cz",
		"release": "standard-version",
		"releaseVersion": "./versionRelease.sh"
	},
	"dependencies": {
		"@nestjs/common": "^11.0.1",
		"@nestjs/config": "^4.0.2",
		"@nestjs/core": "^11.0.1",
		"@nestjs/event-emitter": "^3.0.1",
		"@nestjs/jwt": "^11.0.0",
		"@nestjs/microservices": "^11.1.3",
		"@nestjs/passport": "^11.0.5",
		"@fastify/cookie": "^11.0.2",
		"@nestjs/platform-fastify": "^11.0.1",
		"@nestjs/typeorm": "^11.0.0",
		"@types/pdfmake": "^0.2.11",
		"amqp-connection-manager": "^4.1.14",
		"amqplib": "^0.10.8",
		"argon2": "^0.43.0",
		"axios": "^1.10.0",
		"class-validator": "^0.14.2",
		"cookie-parser": "^1.4.7",
		"dotenv": "^16.5.0",
		"easy-soap-request": "^5.6.1",
		"fast-xml-parser": "^5.2.5",
		"fastify": "^5.7.4",
		"ioredis": "^5.6.1",
		"jsonwebtoken": "^9.0.2",
		"nestjs-zod": "^4.3.1",
		"passport-jwt": "^4.0.1",
		"pdfmake": "^0.2.20",
		"pg": "^8.14.1",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1",
		"soap": "^1.1.12",
		"typeorm": "0.3.24",
		"xml2js": "^0.6.2",
		"xmlbuilder2": "^3.1.1",
		"zod": "^3.25.67"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@commitlint/config-conventional": "^19.8.1",
		"@eslint/eslintrc": "^3.2.0",
		"@eslint/js": "^9.18.0",
		"@nestjs/cli": "^11.0.0",
		"@nestjs/schematics": "^11.0.0",
		"@nestjs/testing": "^11.0.1",
		"@swc/cli": "^0.6.0",
		"@swc/core": "^1.10.7",
		"@types/express": "^5.0.0",
		"@types/cookie-parser": "^1.4.9",
		"@types/jest": "^29.5.14",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "^22.10.7",
		"@types/supertest": "^6.0.2",
		"@typescript-eslint/eslint-plugin": "^8.34.1",
		"@typescript-eslint/parser": "^8.34.1",
		"@vitest/coverage-v8": "^2.1.8",
		"commitizen": "^4.3.1",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-boundaries": "^4.2.1",
		"eslint-plugin-prettier": "^5.5.0",
		"globals": "^16.0.0",
		"jsdom": "^28.1.0",
		"lint-staged": "^16.1.2",
		"prettier": "^3.5.3",
		"source-map-support": "^0.5.21",
		"standard-version": "^9.5.0",
		"supertest": "^7.0.0",
		"ts-loader": "^9.5.2",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.20.0",
		"unplugin-swc": "^1.4.4",
		"vitest": "^2.1.8"
	},
	"config": {
		"owner": "walait",
		"ghooks": {
			"pre-commit": "npm run lint",
			"commit-msg": "commitlint -e --verbose"
		}
	},
	"trustedDependencies": ["@nestjs/core", "@swc/core"]
}
